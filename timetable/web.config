<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <add name="Content-type:" value=" text/html; charset=utf-8" />
            </customHeaders>
        </httpProtocol>
        <caching>
            <profiles>
                <add extension=".php" policy="DontCache" kernelCachePolicy="DontCache" duration="03:00:00" />
            </profiles>
        </caching>
    </system.webServer>
</configuration>
